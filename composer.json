{
    "name": "survos/code-bundle",
    "description": "generate PHP code for Symfony applications. Like maker-bundle, but with nette/php-generator",
    "keywords": [
        "dev",
        "code",
        "generator",
        "php"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.4",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "twig/twig": "^3.4",
        "nette/php-generator": "^4.1",
        "roave/better-reflection": "^6.0",
        "nikic/php-parser": "^5.6"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/var-dumper": "^7.3",
        "symfony/console": "^7.3",
        "symfony/routing": "^7.3",
        "symfony/security-bundle": "^7.3",
        "doctrine/orm": "^2.12 || ^3.3",
        "halaxa/json-machine": "^1.2",
        "league/csv": "^9.9",
        "survos/state-bundle": "^1.5",
        "phpunit/phpunit": "^12",
        "doctrine/doctrine-bundle": "^2.7",
        "symfony/http-client-contracts": "^3.5",
        "survos/babel-bundle": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Survos\\CodeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Survos\\CodeBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "endroid/installer": false
        }
    }
}
